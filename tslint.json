{
    "extends": [
      "tslint:recommended"
    ],
    "rules": {
      "arrow-parens": false,
      "interface-name": false,
      "max-line-length": [
        true,
        120
      ],
      "max-classes-per-file": [false, 1],
      "no-console": [
        true,
        "time",
        "timeEnd",
        "trace"
      ],
      "no-string-literal": false,
      "object-literal-sort-keys": false,
      "ordered-imports": false,
      "quotemark": [
        true,
        "single",
        "avoid-escape"
      ],
      "trailing-comma": [
        false,
        {
          "multiline": "always",
          "singleline": "never"
        }
      ],
      "variable-name": [
        true,
        "allow-leading-underscore",
        "ban-keywords",
        "check-format"
      ]
    }
  }